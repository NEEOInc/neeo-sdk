{
  "name": "neeo-sdk",
  "version": "0.51.4",
  "description": "NEEO Brain SDK. For examples see https://github.com/NEEOInc/neeo-sdk-examples.",
  "repository": "https://github.com/NEEOInc/neeo-sdk",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "bonjour": "^3.5.0",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "fast-url-parser": "^1.1.3",
    "openpgp": "^3.0.13",
    "rxjs": "^6.2.2",
    "semver": "^5.5.0",
    "tokensearch.js": "^0.8.0",
    "tslib": "^1.9.3",
    "validate.js": "^0.12.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/bonjour": "^3.5.5",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.16.0",
    "@types/express-serve-static-core": "^4.16.0",
    "@types/node": "^10.5.2",
    "@types/openpgp": "^0.0.30",
    "@types/semver": "^5.5.0",
    "@types/yargs": "^11.1.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "husky": "^0.14.3",
    "jshint": "^2.9.5",
    "lint-staged": "^7.2.0",
    "lodash.merge": "^4.6.1",
    "mocha": "^5.1.1",
    "nock": "^9.4.2",
    "nyc": "^12.0.2",
    "prettier": "^1.13.7",
    "shx": "^0.3.2",
    "sinon": "^6.1.3",
    "sinon-chai": "^3.1.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "npm run jshint && nyc mocha \"test/**/*.js\"",
    "jshint": "jshint --show-non-errors --verbose lib/**/ test/**/",
    "build": "shx rm -rf ./lib & tsc",
    "clean": "rimraf ./lib && mkdir ./docs",
    "coverage:report": "nyc report text",
    "doc": "shx rm -rf ./docs & typedoc --excludePrivate --excludeExternals --excludeNotExported --hideGenerator --mode file --out ./docs",
    "release": "npm run test && npm run doc && npm run release:copy && npm run release:listtodos",
    "release:copy": "cp -r ./lib ./cli ./test ./dist && cp *.md .jshintrc .gitignore index.js .editorconfig .npmignore .travis.yml package.json License ./dist",
    "release:listtodos": "grep -R --exclude='./node_modules/*' 'TODO' ./dist",
    "prettier": "prettier src/**/*.{ts,json} --write",
    "prepare": "npm run build"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 70,
    "statements": 70,
    "functions": 70,
    "branches": 70,
    "include": [
      "lib/**/*.js",
      "cli/**/*.js"
    ],
    "all": true,
    "reporter": [
      "text-summary",
      "lcov"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "bin": {
    "neeo-sdk": "./lib/cli/index.js"
  }
}
